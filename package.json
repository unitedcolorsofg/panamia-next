{
  "name": "panamia",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "env -u NODE_ENV node server.js",
    "dev:http": "env -u NODE_ENV next dev",
    "build": "drizzle-kit migrate && env -u NODE_ENV next build",
    "build:next": "env -u NODE_ENV next build",
    "build:cf": "drizzle-kit migrate && opennextjs-cloudflare build",
    "deploy:cf": "opennextjs-cloudflare deploy",
    "build:cf:next": "env -u NODE_ENV next build && opennextjs-cloudflare build",
    "postbuild": "next-sitemap",
    "start": "env -u NODE_ENV next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "prepare": "husky && (git remote get-url activities-upstream 2>/dev/null || git remote add activities-upstream https://github.com/llun/activities.next.git)",
    "subtree:pull": "git subtree pull --prefix=external/activities.next activities-upstream main --squash",
    "env:check": "npx tsx scripts/env-check.ts check",
    "env:workflow": "npx tsx scripts/env-check.ts workflow",
    "env:list": "npx tsx scripts/env-check.ts list",
    "env:secrets": "npx tsx scripts/env-check.ts secrets",
    "env:vars": "npx tsx scripts/env-check.ts vars",
    "db:reset": "npx tsx scripts/reset-test-db.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
